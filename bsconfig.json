{
  "name": "connections-custom",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "test",
      "type": "dev",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "es6",
      "in-source": true
    }
  ],
  "suffix": ".bs.js",
  "jsx": {
    "version": 4,
    "mode": "automatic"
  },
  "bs-dependencies": [
    "@rescript/react",
    "@rescript/core",
    "rescript-funicular",
    "rescript-webapi"
  ],
  "bs-dev-dependencies": [
    "rescript-vitest",
    "rescript-react-testing-library"
  ],
  "bsc-flags": [
    "-open RescriptCore"
  ]
}
